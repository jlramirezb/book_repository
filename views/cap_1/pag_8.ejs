  <style>
    .formElement {
      margin-top: 10px;
      border: 3px solid #e7e6e6;
      background-color: #fefefe;

    }

    .boardElement {
      width: fit-content;

      display: block;
      margin: auto;
      display: flex;
      justify-content: center;
    }

    .containerCard {
      display: flex;
      padding: 0;

      margin-bottom: 50px;
      width: 45%;

    }

    .card {
      border: 1px solid var(--azulMedio);
      height: max-content;
      width: 100%;
      box-sizing: border-box;
      border-radius: 10px;
    }

    .card-body {
      padding-bottom: 0;
      margin-bottom: 0;

    }

    .card-footer {
      margin: 0;
      border: 1px solid var(--azulMedio);
      border-radius: 9px;
    }

    td div {
      font-weight: 700;
    }

    #artifact-container {
      width: 100%;
      background-color: #fefefe;
      border-radius: 10px;
      letter-spacing: 1px;
    }

    .artifact-container {
      width: 100%;
      border: 1px solid var(--azulMedio);
      border-radius: 9px;
      letter-spacing: 1px;
    }

    @media only screen and (max-width: 900px) {
      .boardElement {
        width: 300px;
      }

      .containerCard {
        width: 90%;
        font-size: 16px;
      }

      .containerNormal {
        justify-content: center;
      }

      .toValidate {
        margin: 0;
      }
    }

    @media only screen and (max-width: 400px) {
      label {
        font-size: 10px;
      }

      .boardElement {
        width: 200px;
      }

      .containerCard {
        width: 100%;
        padding: 0;
        margin: 0;
        margin-bottom: 50px;
        text-align: left;
      }

      td {
        width: 1px;
        text-align: left;

      }

      tr {}


    }

    @media only screen and (max-width: 310px) {
      .card {
        overflow: hidden;
        box-sizing: border-box;
        padding: 10px;
      }

      tr {
        opacity: 1;
      }

      #content {
        width: 250px !important;
        overflow-x: scroll;
        font-size: 10px;

      }

      #artifact-container {

        background-color: #fff;
        letter-spacing: normal;
      }
    }
  </style>
