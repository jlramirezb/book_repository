<%- include(rutes+'/head.min.ejs'); %>
  <template id="tmp-road"><!--  template engine road -->
    <div id="artifacts" class="d-flex flex-column borderDefault m-2 mt-5 squares-div">
      <div class="defRoadBoard jxgbox mb-1 boardResp" data-board="board_0" data-artifact="artifact_1"></div>
      <div class="d-flex flex-wrap justify-content-center border border-dark rounded btn-all">
        <button class="visible button-marg buttonTertiary buttonKey" title="Visualizar oculatar"></button>
        <button class="finishRoad button-marg buttonTertiary buttonKey" title="Completar punto final"></button>
        <button class="road button-marg buttonTertiary buttonKey" title="Camino"></button>
        <button class="curve button-marg buttonTertiary buttonKey" title="Crear curva con los puntos finales"></button>
        <button class="pointClose button-marg buttonTertiary buttonKey" title="Punto para la curva"></button>
        <div>
          <button class="help buttonSecundary button-marg  buttonKey" title="Ayuda"></button>
          <button class="back buttonSecundary button-marg  buttonKey" title="Retornar"></button>
          <button class="reset button-marg buttonSecundary buttonKey" title="Reiniciar"></button>
          <button class="check button-marg buttonPrimary buttonKey" title="Validar"></button>
        </div>
      </div>
    </div>
  </template> <!-- fin template road-->

  <template id="tmp-sym"> <!-- template engine simetria -->

    <div id="artifacts" class="d-flex flex-column borderDefault m-2 mt-5 squares-div">
      <!--  style="width: 700px; height: 600px; max-width: 600px; max-height: 850px;" -->
      <div class="defRoadBoard jxgbox mb-1 boardResp" data-board="board_0" data-artifact="artifact_1"></div>
      <div class="d-flex flex-wrap justify-content-center border border-dark rounded btn-all">

        <div class="interact-btn">
          <button data-axie="axieX" class="axie buttonSecundaryMod button-marg buttonKey" style="color: #4afdff;">Eje
            X</button>
          <button data-axie="axieY" class="axie buttonSecundaryMod button-marg buttonKey" style="color: #6efc6e;">Eje
            Y</button>
          <button data-axie="axieB" class="axie buttonSecundaryMod button-marg buttonKey" style="color: #ff5132;">
            Bisectriz </button>
        </div>
        <div class="d-flex align-items-center">
          <input type="text" name="" id="inpAxie" style="height: 80%; width: 100px; margin-left: 2px;">
          <button class="reset button-marg buttonSecundary buttonKey"></button>
          <button class="check button-marg buttonPrimary buttonKey"></button>
        </div>
      </div>
    </div>
  </template><!-- fin template simetria-->
  <div class="containerNormal background-gray">
    <h3>Operaciones con curvas</h3>
    <hr>
    <div class="d-flex justify-content-center flex-column">
      <div id="mainOwner" class="d-flex flex-wrap justify-content-center" style="max-width: 1500px; margin: 0;">
      </div>
      <div id="grid" class="d-flex flex-wrap justify-content-center" style="max-width: 1500px; margin: 0;">
      </div>
      <template id="template">
        <div class="content_1">
          <div class="content_2">
          </div>
        </div>
      </template>
    </div>
  </div>

  <!-- template para operation curves -->
  <template id="tmp-SRP">
    <div id="artifacts" class="d-flex flex-column borderDefault m-2 squares-div">

      <div class="defRoadBoard jxgbox mb-1 boardResp" data-board="board_0" data-artifact="artifact_1"> </div>

      <div class="d-flex w-100 flex-wrap justify-content-center border border-dark rounded btn-all">
        <div class="d-flex flex-row">
          <div class="d-flex flex-column">
            <div class="d-flex flex-wrap justify-content-center">

              <button class="curve button-marg buttonTertiary buttonKey buttonAux"
                title="Crear curva con los puntos finales"></button>
              <button class="pointClose button-marg buttonTertiary buttonKey buttonAux"
                title="Punto para la curva"></button>
              <button class="btnAsymptotes button-marg buttonTertiary buttonKey buttonAux" title="Asintota"></button>
            </div>
            <div class="d-flex flex-wrap">
              <button class="help buttonSecundary button-marg  buttonKey" title="Ayuda"></button>
              <button class="gear buttonSecundary button-marg  buttonKey"> </button>
              <button class="infinite buttonSecundary button-marg  buttonKey"> </button>
              <button class="back buttonSecundary button-marg  buttonKey" title="Retornar"></button>
              <button class="reset button-marg buttonSecundary buttonKey" title="Reiniciar"></button>
              <button class="check button-marg buttonPrimary buttonKey" title="Validar"></button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </template>


  <!-- fin templates necesarios para opreration curves -->
  <!-- comienza templates raul para el motor de jorman -->
  <template id="globalTemplate">
    <div id="artifact-container"
      class="d-flex w-100 flex-wrap justify-content-center rounded btn-all artifact-container">
      <div id="content" class="">
      </div>
      <div class="d-flex w-100 flex-wrap justify-content-center  rounded btn-all">
        <div class="d-flex flex-row">
          <div class="d-flex flex-wrap justify-content-start"></div>
          <button class="reset button-marg buttonSecundary buttonKey" title="Reiniciar"></button>
          <button class="check button-marg buttonPrimary buttonKey" title="Validar"></button>
        </div>
      </div>
    </div>
  </template>

  <template id="templateSelect">
    <div class='row flex-wrap mt-2' id="selectq">
      <div class="col-lg-6 col-md-12 col-sm-12">
        <label for="opciones"></label>
      </div>
      <div class="col-lg-6 col-md-12 col-sm-12  d-flex justify-content-end">
        <select class="form-select form-control" name="opciones" id="opciones" style="width:100%;max-width:90%;">
      </div>
      </select>
    </div>
  </template>

  <template id="templateCheck">
    <div class=" row d-flex" id="radioContainer">
      <div class="title col-4 justify-content-start"> </div>
      <div class="content col-6 justify-content-start"> </div>
    </div>
  </template>

  <template id="templateInput">
    <div class="row">
      <div id="title" class="d-flex justify-content-start col-12 "> </div>
      <div id="content" class="d-flex justify-content-between col-12"></div>
    </div>
    </div>
