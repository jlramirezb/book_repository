    <!-- 
         Si se quiere agregar mas artefactos deben estar agregada la etiqueta y la definicion
      <div class="elementEtw" data-board="board_1"></div>

      class: campo obligatorio. xxxxx No eliminar xxx
      data-board: contiene el board declarado en 'etwDefBoards' que se usuara en el el artefacto
       -->

    <!-- </div> -->

    <!-- Inicio template que utiliza el motor-->

    <template id="tmp-way">
      <div id="artifact-" class="etwArtifact">
        <div class="etwArtifact__title"></div>
        <div class="etwArtifact__Board etwBoard jxgbox" data-board="board_" data-artifact="artifact_"></div>

        <div class="etwArtifact__elements"></div>

        <div class="etwArtifact__btnContainer btn-all">
          <div>
            <button class="help-btn button-marg buttonSecundary buttonKey d-none"></button>
            <button class="reset button-marg buttonSecundary buttonKey"></button>
            <button class="etwArtifact__btnValidate check button-marg buttonPrimary buttonKey"></button>
          </div>
        </div>
      </div>
    </template>
